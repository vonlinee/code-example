<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!--https://blog.csdn.net/weixin_44368212/article/details/109283097-->
<generatorConfiguration>
    <!--加载外部属性配置文件-->
    <!--    <properties resource="db.properties"/>-->
    <!--加载文件-->
    <!--    <classPathEntry location="/Program Files/IBM/SQLLIB/java/db2java.zip"/>-->
    <context id="mysqlTables" targetRuntime="MyBatis3">
        <!--插件-->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <!--自定义插件-->
        <plugin type="samples.plugins.WebMVCPlugin">
            <property name="name" value="孙允珠"/>
            <property name="age" value="30"/>
        </plugin>

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/db_mysql?useUnicode=true&amp;characterEncoding=utf8&amp;useSSL=false&amp;serverTimezone=GMT%2B8"
                        userId="root"
                        password="123456">
        </jdbcConnection>
        <!--类型解析器-->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!--实体类-->
        <javaModelGenerator targetPackage="test.model" targetProject="D:/MBGTestProject/src">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!--Mapper-->
        <sqlMapGenerator targetPackage="test.xml" targetProject="D:/MBGTestProject/src">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!--客户端生成器-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="test.dao" targetProject="D:/MBGTestProject/src">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!--生成的表配置 -->
        <!-- schema：数据库名  tableName：表名  -->
        <table schema="db_mysql" tableName="course" domainObjectName="Course" enableCountByExample="false"
               enableDeleteByExample="false" enableInsert="true" enableSelectByExample="false" mapperName="UserMapper">
            <property name="useActualColumnNames" value="true"/>
            <!--主键-->
            <generatedKey column="USER_ID" sqlStatement="MySql" identity="true"/>
            <!--            <columnOverride column="DATE_FIELD" property="startDate"/>-->
            <!--            <ignoreColumn column="FRED"/>-->
            <!--            <columnOverride column="LONG_VARCHAR_FIELD" jdbcType="VARCHAR"/>-->
        </table>
    </context>
</generatorConfiguration>