<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<SplitPane fx:id="splitpane" dividerPositions="1.0" prefHeight="800.0" prefWidth="1200.0"
           xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="io.devpl.codegen.mbg.fx.controller.MainPageController">
    <styleClass>
        <String fx:value="bg"/>
        <String fx:value="trans"/>
    </styleClass>

    <StackPane fx:id="editpane" prefHeight="150.0" prefWidth="200.0" styleClass="trans"
               BorderPane.alignment="CENTER">
        <SplitPane dividerPositions="0.99" orientation="VERTICAL" prefHeight="200.0"
                   prefWidth="160.0" styleClass="trans">
            <BorderPane fx:id="codepane" maxWidth="-Infinity" prefHeight="200.0" prefWidth="10240.0"
                        BorderPane.alignment="CENTER">

            </BorderPane>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0"/>
        </SplitPane>
        <ToggleButton fx:id="slider" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                      minWidth="-Infinity" mnemonicParsing="false" prefHeight="32.0" prefWidth="25.0"
                      style="-fx-background-color: rgba(0,0,0,0);" StackPane.alignment="CENTER_RIGHT">
            <StackPane.margin>
                <Insets bottom="49.0" right="10.0"/>
            </StackPane.margin>
        </ToggleButton>
        <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
              prefHeight="27.0" prefWidth="160.0" StackPane.alignment="TOP_RIGHT">
            <StackPane.margin>
                <Insets right="16.0" top="12.0"/>
            </StackPane.margin>
            <Button mnemonicParsing="false" prefWidth="49.0" text="D"/>


            <ToggleButton mnemonicParsing="false" prefHeight="32.0" prefWidth="54.0" text="F"/>
            <Button mnemonicParsing="false" prefWidth="49.0" text="S"/>
        </HBox>
    </StackPane>


    <SplitPane fx:id="funcpane" dividerPositions="0.5" minWidth="0.0" orientation="VERTICAL" prefWidth="0">
        <VBox fx:id="menupane">
            <Button fx:id="open" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                    minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                    onAction="#openaction" prefHeight="27.0" prefWidth="81.0" text="open"/>
            <Button fx:id="saveit" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                    minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                    onAction="#saveitaction" prefHeight="27.0" prefWidth="81.0" text="save it"/>
            <Button fx:id="saveas" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                    minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                    onAction="#saveasaction" prefHeight="27.0" prefWidth="81.0" text="save as"/>
            <Button fx:id="gentime" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                    minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#genTime"
                    prefHeight="27.0" prefWidth="81.0" text="time/C+T"/>
            <TextField fx:id="input" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                       minHeight="-Infinity" minWidth="-Infinity" prefHeight="27.0" prefWidth="160.0"
                       styleClass="search" text="input keywords">
            </TextField>
            <Button fx:id="search" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                    minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                    onAction="#findsearch" prefHeight="25.0" prefWidth="25.0"/>
            <Button fx:id="replace" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                    minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                    onAction="#findreplace" prefHeight="25.0" prefWidth="25.0"/>
            <Hyperlink fx:id="githublink" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                       minHeight="-Infinity" minWidth="-Infinity" onAction="#visitgithub" prefHeight="25.0"
                       prefWidth="145.0" text="Detail consult readme file" textAlignment="CENTER"
                       textFill="#66003a" underline="true">
            </Hyperlink>
            <Label fx:id="viewer" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                   minHeight="-Infinity" minWidth="-Infinity" prefHeight="16.0" prefWidth="100.0"
                   text="CopyrightÂ© supported by blinderjay, all rights reserved" textAlignment="CENTER"/>
            <ChoiceBox fx:id="choosebox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                       minWidth="-Infinity" nodeOrientation="LEFT_TO_RIGHT" onAction="#chooseMode"
                       prefHeight="18.0" prefWidth="81.0"/>
            <TreeView fx:id="treelist" minWidth="0" prefHeight="798.0" prefWidth="0.0"/>
        </VBox>
        <BorderPane fx:id="consolepane">
        </BorderPane>
    </SplitPane>

</SplitPane>
