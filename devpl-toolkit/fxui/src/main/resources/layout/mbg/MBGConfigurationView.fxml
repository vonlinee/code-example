<?xml version="1.0" encoding="UTF-8"?>

<!--MyBatis Generator 配置-->

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane prefHeight="290.0" prefWidth="622.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="io.devpl.toolkit.fxui.controller.mbg.MBGConfigurationView">

    <columnConstraints>
        <ColumnConstraints halignment="RIGHT" hgrow="NEVER"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="688.0"/>
        <ColumnConstraints halignment="LEFT" hgrow="NEVER"/>
        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES"/>
        <ColumnConstraints halignment="RIGHT" hgrow="NEVER"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="ALWAYS"/>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="ALWAYS"/>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="ALWAYS"/>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="ALWAYS"/>
        <RowConstraints maxHeight="30.0" minHeight="0.0" prefHeight="0.0" vgrow="ALWAYS"/>
        <RowConstraints maxHeight="90.0" minHeight="10.0" prefHeight="90.0" valignment="CENTER" vgrow="NEVER"/>
        <RowConstraints valignment="CENTER" vgrow="NEVER"/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
    </rowConstraints>
    <Label text="项目根目录" GridPane.rowIndex="0"/>
    <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
        <TextField fx:id="projectFolderField" prefHeight="23.0" prefWidth="202.0"/>
        <Button mnemonicParsing="false" onAction="#chooseProjectFolder" text="选择"/>
    </HBox>
    <Label text="父包名" GridPane.rowIndex="1"/>
    <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <TextField fx:id="txfParentPackageName">
        </TextField>
    </HBox>
    <!--第5行-->
    <Label text="实体类名包名" GridPane.rowIndex="2"/>
    <TextField fx:id="modelTargetPackage" GridPane.columnIndex="1" GridPane.rowIndex="2">
    </TextField>
    <Label text="存放目录" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
    <TextField fx:id="modelTargetProject" text="src/main/java" GridPane.columnIndex="3" GridPane.rowIndex="2">
    </TextField>
    <Label text="Mapper接口包名" GridPane.rowIndex="3"/>
    <TextField fx:id="txfMapperPackageName" promptText="Mapper接口包名,例如com.example.mapper" GridPane.columnIndex="1"
               GridPane.rowIndex="3">
    </TextField>
    <Label text="存放目录" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
    <TextField fx:id="daoTargetProject" promptText="src/main/java" text="src/main/java" GridPane.columnIndex="3"
               GridPane.columnSpan="2" GridPane.rowIndex="3">
    </TextField>
    <!--映射XML文件包名-->
    <Label text="映射XML文件包名" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
    <TextField fx:id="mapperTargetPackage" GridPane.columnIndex="1" GridPane.rowIndex="4">
    </TextField>
    <Label text="存放目录" GridPane.columnIndex="2" GridPane.rowIndex="4"/>
    <TextField fx:id="mappingTargetProject" text="src/main/resources" GridPane.columnIndex="3" GridPane.rowIndex="4">
    </TextField>


    <GridPane GridPane.columnSpan="4" GridPane.rowIndex="5">
        <columnConstraints>
            <ColumnConstraints hgrow="NEVER"/>
            <ColumnConstraints hgrow="NEVER"/>
            <ColumnConstraints/>
            <ColumnConstraints/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
        </rowConstraints>

        <CheckBox fx:id="chbUseExample" minWidth="100.0" mnemonicParsing="false" text="使用Example"
                  GridPane.rowIndex="4"/>
        <CheckBox fx:id="chbOffsetLimit" disable="true" minWidth="100.0" mnemonicParsing="false" selected="true"
                  text="分页插件" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <CheckBox fx:id="chbComment" mnemonicParsing="false" selected="true" text="生成实体域注释(来自表注释)"
                  GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <CheckBox fx:id="chbOverrideXML" mnemonicParsing="false" selected="true" text="覆盖原XML"
                  GridPane.columnIndex="2" GridPane.rowIndex="4"/>
        <CheckBox fx:id="chbUseLombokPlugin" mnemonicParsing="false" text="LombokPlugin" GridPane.rowIndex="5"/>
        <CheckBox fx:id="chbNeedToStringHashcodeEquals" mnemonicParsing="false" selected="true"
                  text="生成toString/hashCode/equals方法" GridPane.columnIndex="3" GridPane.rowIndex="4"/>
        <CheckBox fx:id="chbUseSchemaPrefix" mnemonicParsing="false" text="使用Schema前缀" GridPane.columnIndex="1"
                  GridPane.rowIndex="5"/>
        <CheckBox fx:id="chbForUpdate" mnemonicParsing="false" text="select 增加ForUpdate" GridPane.columnIndex="2"
                  GridPane.rowIndex="5"/>
        <CheckBox fx:id="chbAnnotationDao" mnemonicParsing="false" text="DAO使用 @Repository 注解"
                  GridPane.columnIndex="2" GridPane.rowIndex="9"/>
        <CheckBox fx:id="chbMapperExtend" mnemonicParsing="false" prefHeight="17.0" prefWidth="145.0"
                  text="DAO方法抽出到公共父接口" GridPane.rowIndex="9"/>
        <CheckBox fx:id="chbJsr310Support" mnemonicParsing="false" prefHeight="16.0" prefWidth="252.0"
                  text="JSR310: Date and Time API" GridPane.columnIndex="3" GridPane.rowIndex="9"/>
        <CheckBox fx:id="annotationCheckBox" mnemonicParsing="false" text="生成JPA注解" GridPane.columnIndex="3"
                  GridPane.rowIndex="5"/>
        <CheckBox fx:id="useActualColumnNamesCheckbox" mnemonicParsing="false" text="使用实际的列名"
                  GridPane.columnIndex="3" GridPane.rowIndex="3"/>
        <CheckBox fx:id="useTableNameAliasCheckbox" mnemonicParsing="false" text="启用as别名查询"
                  GridPane.columnIndex="2" GridPane.rowIndex="3"/>
        <CheckBox fx:id="addMapperAnnotationChcekBox" prefHeight="31.0" prefWidth="103.0" text="添加@Mapper注解"
                  GridPane.rowIndex="3"/>
        <CheckBox fx:id="chbEnableSwagger" text="开启Swagger注解" GridPane.columnIndex="1" GridPane.rowIndex="9"/>
    </GridPane>
</GridPane>
